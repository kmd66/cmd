<!DOCTYPE html>
<html dir="rtl" lang="fa-IR">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="~/" />
    <link rel="icon" type="image/png" href="favicon.png" />
    <link rel="stylesheet" href="~/css/bootstrapRtl.css" />


    <script src="~/js/jquery.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/jquery.particleground.min.js"></script>


    <link rel="stylesheet" href="~/css/bundleAlert.css" />
    <link rel="stylesheet" href="~/css/bundleColors.css" />
    <link rel="stylesheet" href="~/css/bundleEditor.css" />
    <link rel="stylesheet" href="~/css/bundleSite.css" />
    <link rel="stylesheet" href="~/css/bundleSlide.css" />
    <link rel="stylesheet" href="~/css/bundleSvg.css" />
    <link rel="stylesheet" href="~/css/bundleXzoom.css" />
    
    <link rel="stylesheet" href="~/css/pSite.css" />


    <script src="~/js/bundleAlert.js"></script>
    <script src="~/js/bundlePsite.js"></script>
    <script src="~/js/bundleSetup.js"></script>
    <script src="~/js/bundleSite.js"></script>
    <script src="~/js/bundleSlide.js"></script>
    <script src="~/js/bundleXzoom.min.js"></script>
    <script src="~/js/bundleEnum.js" ></script>

</head>
<body>
    <cache expires-after="@TimeSpan.FromMinutes(40)">
        @await Component.InvokeAsync("OutsideHeder")
    </cache>
    @RenderBody()
    <cache expires-after="@TimeSpan.FromMinutes(30)">
        @await Component.InvokeAsync("OutsideFotter")
    </cache>

    <div class="loding" id="loding">
        <div class="container h-100">

            <div class="d-flex justify-content-center align-items-center vh-100">
                <div class="spinner-grow" style="width: 3rem; height: 3rem;" role="status">
                    <span class="visually-hidden">Loading...</span>
                </div>
            </div>
        </div>
    </div>
    <div class="anotherElement"></div>
</body>
</html>